# INPUT: Spannung, Stromstärke
# OUTPUT: Benötigter Widerstand
schaltung1 <- function(U0, I0){
# Randbedingungen
R1 = 0 # Widerstand Amperemeter
R2 = 10e6 # Widerstand Voltmeter
# Berechnung des Gesamtwiderstandes
Rges = U0/I0
# Berechnung des benötigten R
R3 = (1/(Rges-R1)-1/R2)**-1
return(R3)
}
